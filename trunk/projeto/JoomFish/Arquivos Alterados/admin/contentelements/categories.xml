<joomfish type="contentelement">
  <name>Categorias</name>
  <author>A. Kempkens</author>
  <version>2.0.3</version>
  <description>Definição para as categorias padrão</description>
  <reference>
  	<table name="categories">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="title" translate="1" >Título</field>
  		<field type="text" name="name" translate="0">Nome</field>
  		<field type="text" name="alias" translate="1" posthandler="filterTitle">Apelido</field>
  		<field type="htmltext" name="description" translate="1" ebuttons="readmore,pagebreak">Descrição</field>
  		<field type="checked_out_by" name="checked_out" translate="0">Última modificação por</field>
  		<field type="checked_out_date" name="checked_out_time" translate="0">Última modificação em</field>
  	</table>
  </reference>
  <translationfilters>
	  <section>section</section>
  	  <published>published</published>
  </translationfilters>
</joomfish>
