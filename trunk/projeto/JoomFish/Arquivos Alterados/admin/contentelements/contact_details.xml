<joomfish type="contentelement">
  <name>Contato</name>
  <author>A. Kempkens</author>
  <version>2.0.3</version>
  <description>Definição para o componente padrão de Contatos</description>
  <reference>
  	<table name="contact_details">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="name" translate="1">Nome</field>
  		<field type="text" name="alias" translate="1" posthandler="filterName">Apelido</field>
  		<field type="text" name="con_position" translate="1">Posição</field>
  		<field type="text" name="email_to" translate="1">e-mail</field>
  		<field type="text" name="address" translate="1">Endereço</field>
  		<field type="text" name="suburb" translate="1">Cidade</field>
  		<field type="text" name="state" translate="1">Estado</field>
  		<field type="text" name="country" translate="1">País</field>
  		<field type="text" name="postcode" translate="1">Código postal</field>
  		<field type="text" name="telephone" translate="1">Telefone</field>
  		<field type="text" name="fax" translate="1">Fax</field>
  		<field type="textarea" name="misc" translate="1">Outras Informações</field>
  		<field type="textarea" name="params" translate="1">Parâmetros</field>
  		<field type="checked_out_by" name="checked_out" translate="0">Última modificação por</field>
  		<field type="checked_out_date" name="checked_out_time" translate="0">Última modificação em</field>
  	</table>
  </reference>
  <translationfilters>
  	  <category>catid</category>
  </translationfilters>
</joomfish>
