<joomfish type="contentelement">
  <name>Módulos</name>
  <author>A. Kempkens</author>
  <version>2.0.3</version>
  <description>Definição geral para o gerenciamento de módulos</description>
  <reference>
  	<table name="modules">
  		<field type="referenceid" name="id" translate="0">ID</field>
  		<field type="titletext" name="title" translate="1">Título</field>
  		<field type="htmltext" name="content" translate="1" ignoreifblank="1">Conteúdo</field>
  		<field type="text" name="module" translate="0">Modulo</field>
  		<field type="params" name="params" translate="1">Parâmetros</field>
  		<field type="checked_out_by" name="checked_out" translate="0">Última modificação por</field>
  		<field type="checked_out_date" name="checked_out_time" translate="0">Última modificação em</field>
  		<filter>c.client_id = 0 </filter>
  	</table>
  </reference>
    <translationfilters>
  	  <keyword>title</keyword>
  	  <published>published</published>
  	  <module>access</module>
    </translationfilters>
</joomfish>
