      	<option value="0">Sem imagem</option>
        <option value="1">Ícone de Pasta</option>
        <option value="2">Miniatura</option>
    </param>
    <param name="doc_image" type="list" default="1" label="Imagem do Documento">
       <option value="0">Sem imagem</option>
       <option value="1">Ícone de Arquivo</option>
       <option value="2">Miniatura</option>
    </param>

