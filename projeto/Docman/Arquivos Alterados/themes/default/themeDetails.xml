        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>
    <param name="details_description" type="radio" default="1" label="Descrição">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>
    <param name="details_filename" type="radio" default="1" label="Nome do arquivo">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>
    <param name="details_filesize" type="radio" default="1" label="Tamanho do Arquivo">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>
    <param name="details_filetype" type="radio" default="1" label="Tipo do Arquivo">
