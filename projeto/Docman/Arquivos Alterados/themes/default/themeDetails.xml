   <param name="@heading" type="heading" label="Geral" description="" />
   <param name="cat_empty" type="radio" default="1" label="Categorias Vazias">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
   </param>

   <param name="@heading" type="heading" label="Menu" description="" />
   <param name="menu_home" type="radio" default="1" label="Entrada Downloads">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>
    <param name="menu_search" type="radio" default="1" label="Pesquisar Documentos">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>
    <param name="menu_upload" type="radio" default="1" label="Upload">
        <option value="0">Esconder</option>
        <option value="1">Mostrar</option>
    </param>

